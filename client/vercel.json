{
    "functions": {
        "api/index.js": {
            "runtime": "@vercel/node@2.15.10"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}